#include <iostream>
#include <cstring>
 
using namespace std;
 
struct Books {
   char  title[50];
   char  author[50];
   int   book_id;
   
   
   void book (string title, string author, int book_id) {
    cout << "Book title : " << title <<endl;
    cout << "Book author : " << author <<endl;
    cout << "Book id : " << book_id <<endl;
    cout << endl;
   }
};
 
int main() {
   struct Books Book1;     
   struct Books Book2;  
   
   Book1.book("afa", "fdfd", 34); 
   Book2.book("afsa", "fdsdfd", 334);

   return 0;
}
